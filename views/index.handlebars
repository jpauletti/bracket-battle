<!-- create cards -->
<div class="container">
    <div class="row">
{{!-- first card --}}
      <div class="col-4">
      <ul>
          {{#each brackets}}
          <li>
              <div class="card" style="width: 18rem">
                  <div class="card-body">
                      <h5 class="card-title">{{brackets.bracket_name}}</h5>
                      <a href="/bracket/{{brackets.id}}" class="card-link">Link</a>
                  </div>
              </div>
          </li>
          {{/each}}
      </ul>
      </div>
    </div>  
  </div>

    

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>
