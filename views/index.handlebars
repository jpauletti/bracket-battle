<!-- create cards -->
<div class="row">
{{!-- first card --}}
    <div class="col-4">
        <ul>
            {{#each brackets}}
            <li>
                <div class="card" style="width: 18rem">
                    <div class="card-body">
                        <h5 class="card-title">{{this.dataValues.bracket_name}}</h5>
                        <a href="/bracket/{{this.dataValues.id}}" class="card-link">Link</a>
                    </div>
                </div>
            </li>
            {{/each}}
        </ul>
    </div>
</div>

    

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>
